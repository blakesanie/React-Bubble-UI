    "@types/styled-components": "^5.1.25",
    "husky": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^7.0.8",
    "styled-components": "^5.3.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "typescript-plugin-css-modules": "^3.4.0"
  }
